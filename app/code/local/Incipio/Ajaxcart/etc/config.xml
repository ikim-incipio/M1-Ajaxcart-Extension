<?xml version="1.0"?>
<config>
	<modules>
		<Incipio_Ajaxcart>
			<version>1.0.1</version>
		</Incipio_Ajaxcart>
	</modules>
	<frontend>
		<routers>
			<routeurfrontend>
				<use>standard</use>
				<args>
					<module>Incipio_Ajaxcart</module>
					<frontName>ajaxcart</frontName>
				</args>
			</routeurfrontend>
		</routers>
		<layout>
            <updates>
                <ajaxcart>
                    <file>ajaxcart.xml</file>
                </ajaxcart>
            </updates>
        </layout>
	</frontend>
</config>